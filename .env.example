# Configurações do servidor
PORT=3000
NODE_ENV=development

# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key
DATABASE_URL=your_database_url
DIRECT_URL=your_direct_url

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# URLs para testes
BOOKS_BASE_URL=https://your-books-storage.com/books
TEST_BOOK_URLS=https://your-books-storage.com/books/book1.pdf,https://your-books-storage.com/books/book2.epub

# OpenAI (para embeddings)
OPENAI_API_KEY=your_openai_api_key

# Configurações de Segurança
JWT_SECRET=seu-jwt-secret-aqui
TOKEN_EXPIRATION=24h

# Configurações de Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Configurações de Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Instruções:
# 1. Copie este arquivo para .env
# 2. Substitua os valores de exemplo pelos valores reais
# 3. Nunca comite o arquivo .env, apenas este arquivo de exemplo
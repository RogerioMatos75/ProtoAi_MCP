# Configurações do Supabase
SUPABASE_URL=https://<seu-projeto>.supabase.co
SUPABASE_KEY=sua-chave-publica-ou-secreta
SUPABASE_ANON_KEY=sua-chave-anonima

# Configurações do banco de dados
MANIFEST_TABLE=mcp_manifests
TRANSACTIONS_TABLE=mcp_transactions

# Configurações do token $PAi
PAI_TOKEN_CONTRACT=0x0000000000000000000000000000000000000000
PAI_TOKEN_DECIMALS=18
PAI_MIN_AMOUNT=0.00001

# Configurações do servidor
SERVER_PORT=8080
SERVER_HOST=localhost
JWT_SECRET=seu-jwt-secret-aqui
TOKEN_EXPIRATION=24h

# Configurações de cache
CACHE_TTL=3600
CACHE_MAX_SIZE=1000

# Configurações de rate limit
RATE_LIMIT_WINDOW=15m
RATE_LIMIT_MAX_REQUESTS=100

# Configurações de Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Instruções:
# 1. Copie este arquivo para .env
# 2. Substitua os valores de exemplo pelos valores reais
# 3. Nunca comite o arquivo .env, apenas este arquivo de exemplo